pkgname=unicsy_demo
pkgver=0.0
pkgrel=0
pkgdesc="Demo of phone functionality"
url="https://github.com/postmarketOS/osk-sdl"
arch="all"
license="GPL3"
depends=""
makedepends=""
subpackages=""

source="${pkgname}-${pkgver}-${pkgrel}.tar.gz::https://github.com/pavelmachek/unicsy_demo/archive/${pkgver}.tar.gz"

options="!strip"

build() {
}

package() {
    install -D -m755 "${srcdir}/${pkgname}-${pkgver}"/osk-sdl \
        "$pkgdir"/usr/bin/osk-sdl
    install -D -m755 "${srcdir}/${pkgname}-${pkgver}"/osk.conf \
        "$pkgdir"/etc/osk.conf
}
